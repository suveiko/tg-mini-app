{
  "name": "tg-mini-app",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "start:bot": "concurrently \"pnpm start:bot:tsc\" \"pnpm start:bot:node\"",
    "start:bot:tsc": "tsc src/telegram-bot/telegram-bot.ts",
    "start:bot:node": "node src/telegram-bot/telegram-bot.js",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.1",
    "@twa-dev/sdk": "^7.0.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.7",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/telegram-web-app": "^7.8.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "supabase": "^1.187.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
